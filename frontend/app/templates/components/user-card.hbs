{{#paper-card as |card|}}
  {{#card.title as |title|}}
    {{#title.text as |text|}}
      {{#text.headline}}{{model.username}}{{/text.headline}}
      {{#text.subhead}}Large{{/text.subhead}}
    {{/title.text}}
    {{title.media size="lg" src=randomImage alt="random" title="random"}}
  {{/card.title}}
  {{#card.actions}}
    {{#paper-button}}Action 1{{/paper-button}}
    {{#paper-button}}Action 2{{/paper-button}}
  {{/card.actions}}
{{/paper-card}}